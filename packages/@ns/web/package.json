{
  "name": "@ns/web",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "clean:dist": "rimraf dist",
    "ts": "ts-node -r tsconfig-paths/register -P ../../../scripts/tsconfig.json",
    "gen-files": "yarn ts scripts/gen-files",
    "serve": "vue-cli-service serve",
    "build:csr": "vue-cli-service build --report --report-json",
    "build:ssr": "vue-cli-service build --no-clean --ssr",
    "build": "yarn build:csr && yarn build:ssr",
    "lint": "vue-cli-service lint",
    "test": "echo \"No test specified. Skip.\""
  },
  "dependencies": {
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.22",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-pwa": "^3.3.0",
    "@vue/cli-plugin-typescript": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "typescript": "^3.2.4",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
