{
  "name": "@ns/web",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "clean:dist": "rimraf dist",
    "gen-files": "yarn ts scripts/gen-files",
    "env": "echo VUE_APP_WEB_VERSION=$npm_package_version > .env.local",
    "serve": "vue-cli-service serve",
    "build:csr": "vue-cli-service build --report --report-json",
    "build:ssr": "vue-cli-service build --no-clean --ssr",
    "build": "yarn build:csr && yarn build:ssr",
    "lint:scss": "stylelint src/**/*.scss",
    "lint:ts": "tslint -p tsconfig.json",
    "lint": "yarn lint:scss && yarn lint:ts",
    "ts": "ts-node -r tsconfig-paths/register -P ../../../scripts/tsconfig.json"
  },
  "dependencies": {
    "@ns/common": "file:../common",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.3",
    "@vue/cli-plugin-pwa": "^3.5.1",
    "@vue/cli-plugin-typescript": "^3.5.3",
    "@vue/cli-service": "^3.5.3",
    "fibers": "^3.1.1",
    "sass": "^1.17.3",
    "sass-loader": "^7.1.0",
    "typescript": "^3.3.4000",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 8",
    "not op_mini all"
  ]
}
