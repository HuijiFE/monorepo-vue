{
  "name": "@ns/common",
  "version": "0.1.1",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "clean:dist": "rimraf dist",
    "gen-files": "yarn ts scripts/gen-files.ts",
    "env": "echo VUE_APP_COMMON_VERSION=$npm_package_version > .env.local",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "lint:scss": "stylelint src/**/*.scss",
    "lint:ts": "tslint -p tsconfig.json",
    "lint": "yarn lint:scss && yarn lint:ts",
    "ts": "ts-node -r tsconfig-paths/register -P ../../../scripts/tsconfig.json"
  },
  "dependencies": {
    "vue": "^2.6.6",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "fibers": "^3.1.1",
    "sass": "^1.17.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3",
    "vue-template-compiler": "^2.6.6"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
