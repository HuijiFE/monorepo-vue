{
  "name": "@huiji/vue-ssr-utils",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "a series of utils for vue ssr.",
  "keywords": [
    "vue",
    "ssr",
    "seo"
  ],
  "main": "dist/common/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "repository": "https://github.com/HuijiFE/huiji-utils",
  "author": "duduluu <duduluu@outlook.com>",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf dist",
    "gen-files": "yarn ts scripts/gen-files.ts",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "yarn build:cjs && yarn build:esm",
    "lint": "tslint -p tsconfig.json",
    "ts": "ts-node -r tsconfig-paths/register -P ../../../scripts/tsconfig.json"
  },
  "peerDependencies": {
    
  }
}
